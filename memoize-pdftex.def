\let\mmz@mdfivesum\pdfmdfivesum
\let\mmz@primitive\pdfprimitive
\let\ifmmz@absdim\ifpdfabsdim
\def\mmz@if@roughly@equal#1#2{%
  \ifpdfabsdim\dimexpr#1-#2\relax<\mmz@precision\relax
    \expandafter\@firstoftwo
  \else
    \expandafter\@secondoftwo
  \fi
}  
% \quitvmode is pdftex's replacement for \leavevmode (8.21)
\let\mmz@leavevmode\quitvmode
\def\mmz@prepare@memo@page{%
  % pdftex docs insist that these should be set to the standard value of 1 true
  % inch:
  \pdfhorigin 1true in \pdfvorigin 1true in
  % I hope that negative values here don't violate the pdftex instructions.
  \hoffset\dimexpr\mmz@padding@left - \pdfhorigin\relax
  \voffset\dimexpr\mmz@padding@top - \pdfvorigin\relax
  \pdfpagewidth\dimexpr\mmz@padding@left + \wd\mmz@box + \mmz@padding@right\relax
  \pdfpageheight\dimexpr\mmz@padding@top + \ht\mmz@box +
    \dp\mmz@box+ \mmz@padding@bottom\relax
}%
\def\mmz@latex@binary{pdflatex}
